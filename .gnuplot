set terminal wxt background rgb "#011627"
set border lc rgb "#8792a7"
set key textcolor rgb '#8792a7'
set title textcolor rgb '#8792a7'
set tics textcolor rgb "#8792a7"
set xlabel textcolor rgb "#8792a7"
set ylabel textcolor rgb "#8792a7"
set grid lc rgb "#8792a7"

# Color some lines
set linetype 1 lw 2 lc rgb '#21c7a8' pointtype 6
set linetype 2 lw 2 lc rgb '#e39aa6' pointtype 6
set linetype 3 lw 2 lc rgb '#ecea8d' pointtype 6

# Key and border colors
set xlabel "X" textcolor rgb '#cba6f7'
set ylabel "Y" textcolor rgb '#cba6f7'

